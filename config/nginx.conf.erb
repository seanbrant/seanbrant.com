worker_processes 1;
daemon off;


events {
    worker_connections 1024;
}


http {
    include mime.types;
    default_type application/octet-stream;

    server {
            listen <%= ENV['PORT'] %>;
            server_name seanbrant.com;
            return 301 $scheme://www.seanbrant.com$request_uri;
    }

    server {
        listen <%= ENV['PORT'] %>;
        server_name www.seanbrant.com;

        location / {
            root html;
            index index.html index.htm;
        }
    }

}
